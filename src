programa
{
	
	funcao inicio()
	{
		inteiro contador,numero,resultado

		escreva("\n Informe um n√∫mero para ver sua tabuada: ")
		leia(numero)

		limpa()

		para( contador= 1; contador<=10; contador++)
		{
			resultado=numero*contador
			escreva(numero, "X", contador, "=",resultado,"\n")
		}
		

		

	}
}
